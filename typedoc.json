{
  "entryPoints": [
    "packages/*"
  ],
  "entryPointStrategy": "packages",
  "name": "Powertools for AWS Lambda (Typescript) API Reference",
  "readme": "README.md",
  "out": "api",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.json",
    "layers",
    "examples/**"
  ],
  "plugin": ["typedoc-plugin-zod"],
  "skipErrorChecking": true,
  "excludePrivate": true,
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": true
  }
}